{
	"name": "@iconify/utils-cli",
	"type": "module",
	"description": "CLI utilities for working with Iconify icon sets used by various packages.",
	"author": "Vjacheslav Trushkin",
	"version": "2.1.7",
	"license": "MIT",
	"bugs": "https://github.com/iconify/iconify/issues",
	"homepage": "https://iconify.design/docs/libraries/utils-cli/",
	"repository": {
		"type": "git",
		"url": "https://github.com/iconify/iconify.git",
		"directory": "packages/utils-cli"
	},
	"scripts": {
		"test": "node ./bin/svg-css-sprite-generator.mjs --root test-cli",
		"test-custom": "node ./bin/svg-css-sprite-generator.mjs --root test-cli --config custom.ts",
		"lint": "eslint --fix src/**/*.ts",
		"prebuild": "pnpm run lint && pnpm run clean",
		"build": "unbuild && rimraf lib/svg-css-sprite/cli.d.ts && rimraf lib/svg-css-sprite/cli.cjs"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./lib/svg-css-sprite/index.d.ts",
			"require": "./lib/svg-css-sprite/index.cjs",
			"import": "./lib/svg-css-sprite/index.mjs"
		}
	},
	"main": "lib/svg-css-sprite/index.cjs",
	"module": "lib/svg-css-sprite/index.mjs",
	"types": "lib/svg-css-sprite/index.d.ts",
	"bin": {
		"svg-css-sprite-generator": "./bin/svg-css-sprite-generator.mjs"
	},
	"files": [
		"bin",
		"lib"
	],
	"dependencies": {
		"@antfu/install-pkg": "^0.1.1",
		"@antfu/utils": "^0.7.5",
		"@iconify/utils": "workspace:^",
		"cac": "^6.7.14",
		"consola": "^3.2.3",
		"debug": "^4.3.4",
		"colorette": "^2.0.20",
		"local-pkg": "^0.4.3",
		"unconfig": "^0.3.10"
	},
	"devDependencies": {
		"@iconify-json/flat-color-icons": "^1.1.6",
		"@types/debug": "^4.1.8",
		"@types/jest": "^29.5.3",
		"@types/node": "^18.17.1",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-prettier": "^5.0.0",
		"rimraf": "^5.0.1",
		"typescript": "^5.1.6",
		"unbuild": "^1.2.1"
	}
}
